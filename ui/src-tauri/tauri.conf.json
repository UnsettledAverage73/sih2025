{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "productName": "Wipedevs",
    "version": "0.1.0",
    "identifier": "com.tauri.dev",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:3000",
        "frontendDist": "../out"
    },
    "app": {
        "windows": [
            {
                "title": "WipeDevs",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
      },
    "bundle": {
      "active": true,
      "targets": "all",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "../../build/wipe_tool"
      ]
    }
  }
  
